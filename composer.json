{
    "name": "justijndepover/laravel-settings",
    "description": "Store settings in your Laravel application",
    "license": "MIT",
    "authors": [
        {
            "name": "Justijn Depover",
            "email": "justijndepover@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "orchestra/testbench": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Justijndepover\\Settings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Justijndepover\\Settings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Justijndepover\\Settings\\SettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "Justijndepover\\Settings\\Facades\\Settings"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
