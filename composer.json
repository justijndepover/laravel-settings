{
    "name": "justijndepover/laravel-settings",
    "description": "Store settings in your Laravel application",
    "keywords": [
        "justijndepover",
        "laravel",
        "laravel-settings",
        "settings"
    ],
    "homepage": "https://github.com/justijndepover/laravel-settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Justijn Depover",
            "email": "justijndepover@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^6.25|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Justijndepover\\Settings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Justijndepover\\Settings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Justijndepover\\Settings\\SettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "Justijndepover\\Settings\\Facades\\Settings"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
